	<xs:complexType name="MyIdTravelAvailabilityDetailsStandByTraveler">
		<xs:annotation>
			<xs:documentation xml:lang="en">A standby traveler with information about his standby status.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="AirTravelerType">
				<xs:sequence>
					<xs:element name="staffDetails">
						<xs:annotation>
							<xs:documentation xml:lang="en"/>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="staffID" type="StringLength0to64" minOccurs="0">
									<xs:annotation>
										<xs:documentation xml:lang="en">An airline internal ID of the staff member.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="dateOfJoining" type="xs:date" minOccurs="0">
									<xs:annotation>
										<xs:documentation xml:lang="en">The date the staff member joined the carrier.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="filekey" type="OTA_CodeType" minOccurs="0">
									<xs:annotation>
										<xs:documentation xml:lang="en">The filekey associated to the standby booking.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="ticketNumber" type="StringLength0to64" minOccurs="0">
									<xs:annotation>
										<xs:documentation xml:lang="en">The ticketnumber associated with this standby booking.</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="priority" type="xs:string" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="en">The travelers priority regarding his waitlist position.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="compartment" type="MyIdTravelCabinType" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="en">The type of compartment the traveler is listed on.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="staffMember" type="xs:boolean" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="en">True if the traveler is a staff member.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="hireDate" type="xs:date" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="en">The hire date of the traveler if he is staff.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="position" type="xs:integer" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="en">The travelers position on the waitlist.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="MyIdTravelAvailabilityDetailsCheckedInStandByTraveler">
